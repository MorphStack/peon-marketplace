{
  "name": "peon-marketplace",
  "version": "0.0.4",
  "description": "Marketplace validation for peon configurations",
  "main": "dist/validate.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/validate.js",
    "dev": "ts-node src/validate.ts",
    "validate": "npm run build && node dist/validate.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "type-check": "tsc --noEmit",
    "test": "npm run type-check && npm run lint && npm run format:check"
  },
  "keywords": [
    "peon",
    "marketplace",
    "validation"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.4.0",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  }
}
